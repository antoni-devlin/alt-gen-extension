{
  "name": "Alt Gen",
  "description": "An extension to generate alt text for images",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "/media/alt.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
